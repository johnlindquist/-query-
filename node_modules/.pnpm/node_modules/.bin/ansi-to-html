#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/ansi-to-html/0.6.14/node_modules/ansi-to-html/bin/ansi-to-html/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/ansi-to-html/0.6.14/node_modules/ansi-to-html/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/ansi-to-html/0.6.14/node_modules/ansi-to-html/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/ansi-to-html/0.6.14/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/ansi-to-html/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/ansi-to-html/bin/ansi-to-html/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/ansi-to-html/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/ansi-to-html/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ansi-to-html/bin/ansi-to-html" "$@"
else 
  exec node  "$basedir/../ansi-to-html/bin/ansi-to-html" "$@"
fi
