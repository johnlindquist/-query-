#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/cli.js/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules/mime/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/mime/1.6.0/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/mime/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/mime/cli.js/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/mime/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else 
  exec node  "$basedir/../mime/cli.js" "$@"
fi
