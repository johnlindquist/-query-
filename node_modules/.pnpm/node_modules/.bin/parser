#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/7.9.4/node_modules/@babel/parser/bin/babel-parser.js/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/7.9.4/node_modules/@babel/parser/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/7.9.4/node_modules/@babel/parser/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/7.9.4/node_modules/@babel/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/7.9.4/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/parser/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/@babel/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/@babel/parser/bin/babel-parser.js/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/@babel/parser/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/@babel/parser/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/@babel/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
else 
  exec node  "$basedir/../@babel/parser/bin/babel-parser.js" "$@"
fi
