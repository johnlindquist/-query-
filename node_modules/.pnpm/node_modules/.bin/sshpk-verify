#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/sshpk/1.16.1/node_modules/sshpk/bin/sshpk-verify/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/sshpk/1.16.1/node_modules/sshpk/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/sshpk/1.16.1/node_modules/sshpk/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/sshpk/1.16.1/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/sshpk/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/sshpk/bin/sshpk-verify/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/sshpk/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/sshpk/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sshpk/bin/sshpk-verify" "$@"
else 
  exec node  "$basedir/../sshpk/bin/sshpk-verify" "$@"
fi
