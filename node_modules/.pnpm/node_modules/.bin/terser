#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/uglifyjs/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/terser/bin/uglifyjs/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/terser/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules/terser/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../terser/bin/uglifyjs" "$@"
else 
  exec node  "$basedir/../terser/bin/uglifyjs" "$@"
fi
