#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/uncss/0.17.3/node_modules/uncss/bin/uncss/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/uncss/0.17.3/node_modules/uncss/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/uncss/0.17.3/node_modules/uncss/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/uncss/0.17.3/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/uncss/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../uncss/0.17.3/node_modules/uncss/bin/uncss" "$@"
else 
  exec node  "$basedir/../../../../../../uncss/0.17.3/node_modules/uncss/bin/uncss" "$@"
fi
