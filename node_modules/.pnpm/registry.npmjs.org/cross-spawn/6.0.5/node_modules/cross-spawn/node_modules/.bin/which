#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/which/1.3.1/node_modules/which/bin/which/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/which/1.3.1/node_modules/which/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/which/1.3.1/node_modules/which/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/which/1.3.1/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/which/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../which/1.3.1/node_modules/which/bin/which" "$@"
else 
  exec node  "$basedir/../../../../../../which/1.3.1/node_modules/which/bin/which" "$@"
fi
