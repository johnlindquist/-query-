#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/cssesc/3.0.0/node_modules/cssesc/bin/cssesc/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/cssesc/3.0.0/node_modules/cssesc/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/cssesc/3.0.0/node_modules/cssesc/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/cssesc/3.0.0/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/cssesc/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../cssesc/3.0.0/node_modules/cssesc/bin/cssesc" "$@"
else 
  exec node  "$basedir/../../../../../../cssesc/3.0.0/node_modules/cssesc/bin/cssesc" "$@"
fi
