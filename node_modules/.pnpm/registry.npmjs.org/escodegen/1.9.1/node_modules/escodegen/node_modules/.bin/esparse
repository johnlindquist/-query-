#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/esparse.js/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/esprima/3.1.3/node_modules/esprima/bin/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/esprima/3.1.3/node_modules/esprima/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/esprima/3.1.3/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/esprima/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/registry.npmjs.org/node_modules:/Users/johnlindquist/projects/{query}/node_modules/.pnpm/node_modules:/Users/johnlindquist/projects/{query}/node_modules:/Users/johnlindquist/projects/node_modules:/Users/johnlindquist/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../esprima/3.1.3/node_modules/esprima/bin/esparse.js" "$@"
else 
  exec node  "$basedir/../../../../../../esprima/3.1.3/node_modules/esprima/bin/esparse.js" "$@"
fi
